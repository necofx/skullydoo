class VolumeLoaderGUIBase {open
} {
  Function {VolumeLoaderGUIBase()} {open
  } {
    Fl_Window window {
      label {Load Options} open
      xywh {378 57 383 499} type Double hotspot modal visible
    } {
      Fl_Choice comboFileFormats {
        label {Format:} open
        xywh {10 15 360 20} down_box BORDER_BOX align 5
      } {
        menuitem {} {
          label BMP
          callback {fileFormatChanged();}
          xywh {0 0 100 20}
        }
        menuitem {} {
          label JPEG
          callback {fileFormatChanged();}
          xywh {10 10 100 20}
        }
        menuitem {} {
          label TIFF
          callback {fileFormatChanged();}
          xywh {20 20 100 20}
        }
        menuitem {} {
          label PNG
          callback {fileFormatChanged();}
          xywh {30 30 100 20}
        }
        menuitem {} {
          label PPM
          callback {fileFormatChanged();}
          xywh {40 40 100 20}
        }
        menuitem {} {
          label RAW
          callback {fileFormatChanged();}
          xywh {50 50 100 20}
        }
        menuitem {} {
          label {VOLUME-16}
          callback {fileFormatChanged();}
          xywh {60 60 100 20}
        }
        menuitem {} {
          label {HUMAN-PROJECT (Ge SIGNA)}
          callback {fileFormatChanged();}
          xywh {70 70 100 20}
        }
      }
      Fl_Input sepX {
        label {X Spacing:}
        xywh {60 40 35 25} type Float
        code0 {o->value("1.5");}
      }
      Fl_Input sepY {
        label {Y Spacing:}
        xywh {195 40 35 25} type Float
        code0 {o->value("1.5");}
      }
      Fl_Input sepZ {
        label {Sep. Z:}
        xywh {335 40 35 25} type Float
        code0 {o->value("3.2");}
      }
      Fl_Group {} {
        label {The selected file:} open
        xywh {10 85 360 175} box THIN_UP_BOX align 5
      } {
        Fl_Round_Button rbVolumeFile {
          label {Contains the entire volume}
          callback {grVolumeFileOptions->deactivate();
dimZ->activate();}
          xywh {20 90 195 20} type Radio down_box ROUND_DOWN_BOX value 1
        }
        Fl_Round_Button rbSliceFile {
          label {It´s only a slice}
          callback {grVolumeFileOptions->activate();
dimZ->deactivate();}
          xywh {240 90 115 20} type Radio down_box ROUND_DOWN_BOX
        }
        Fl_Group grVolumeFileOptions {open
          xywh {15 110 350 145} box THIN_UP_BOX align 5 deactivate
        } {
          Fl_Input fnamePrefix {
            label {Prefix File name:}
            callback {updateSample();}
            xywh {20 130 340 25} align 5 when 1
          }
          Fl_Input editMask {
            label {Mask:}
            callback {updateSample();}
            xywh {85 165 70 25}
          }
          Fl_Box lblsample {
            label label
            xywh {20 230 340 20} box BORDER_BOX labelsize 9
          }
          Fl_Input sliceSep {
            label {Spacing:}
            xywh {105 195 50 25} type Int
            code0 {o->value("1");}
          }
          Fl_Input sliceNum {
            label {Count:}
            xywh {315 165 45 25} type Int
            code0 {o->value("0");}
          }
          Fl_Input sliceStart {
            label {Initial Offset:}
            xywh {315 195 45 25} type Int
            code0 {o->value("1");}
          }
        }
      }
      Fl_Group grRawFileOptions {open
        xywh {10 262 360 188} box THIN_UP_BOX
      } {
        Fl_Group {} {
          label Dimensions open
          xywh {15 280 205 165} box THIN_UP_BOX align 5
        } {
          Fl_Button btnGuess {
            label Guess
            callback {guessDimensions();}
