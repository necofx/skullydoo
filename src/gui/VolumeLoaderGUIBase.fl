          Fl_Input dimY {
            label {Y:}
            xywh {100 300 35 25} type Int align 5
          }
          Fl_Input dimZ {
            label {Z:}
            xywh {175 300 35 25} type Int align 5
          }
        }
        Fl_Group {} {
          label {Voxel size} open
          xywh {240 282 115 113} box THIN_UP_BOX align 5
        } {
          Fl_Round_Button rb8bit {
            label {8-bits}
            callback {invalidateGuessedDims();}
            xywh {245 290 70 25} type Radio down_box ROUND_DOWN_BOX value 1
          }
          Fl_Round_Button rb12bit {
            label {12-bits}
            callback {invalidateGuessedDims();}
            xywh {245 315 70 25} type Radio down_box ROUND_DOWN_BOX
          }
          Fl_Round_Button rb16bit {
            label {16-bits}
            callback {invalidateGuessedDims();}
            xywh {245 340 75 25} type Radio down_box ROUND_DOWN_BOX
