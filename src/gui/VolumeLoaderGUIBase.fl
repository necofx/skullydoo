        } {
          Fl_Round_Button rbLE {
            label LE
            xywh {245 420 45 20} type Radio down_box ROUND_DOWN_BOX value 1
          }
          Fl_Round_Button rbBE {
            label BE
            xywh {295 420 45 20} type Radio down_box ROUND_DOWN_BOX
          }
        }
      }
      Fl_Check_Button cbConvertToFloat {
        label {Convert to float}
        xywh {20 455 135 25} down_box DOWN_BOX value 1
      }
      Fl_Return_Button btnLoad {
        label Load
        callback {window->user_data((void*)1);
window->hide();}
        xywh {170 455 90 25} shortcut 0xd
      }
      Fl_Button {} {
        label Cancel
        callback {window->user_data((void*)0);
window->hide();}
        xywh {275 455 80 25}
      }
    }
  }
  Function {updateSample()} {open return_type {virtual void}
  } {}
  Function {guessDimensions()} {open return_type {virtual void}
  } {}
  Function {invalidateGuessedDims()} {open return_type {virtual void}
  } {}
  Function {assignGuessed()} {open return_type {virtual void}
  } {}
  Function {fileFormatChanged()} {open selected return_type {virtual void}
  } {}
